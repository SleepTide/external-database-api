{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "Es2022",
    "moduleResolution": "Node",
    "rootDir": "src",
    "outDir": "dist",
    "esModuleInterop": true,
    "removeComments": true,
    "pretty": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noImplicitThis": true
  }
}
